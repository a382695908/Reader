{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "radix": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "react/prefer-stateless-function":0,
    "no-underscore-dangle":0,
    "react/prop-types": 0,
    "semi": [0, "never"],
    "spaced-comment": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "object-shorthan": 0,
    "no-lonely-if": 0,
    "one-var": 0,
    "prefer-const": 0,
    "no-plusplus": 0,
    "camelcase": 0,
    "eqeqeq": 0,
    "react/no-string-refs": 0,
    "class-methods-use-this": 0,
    "react/self-closing-comp": 0,
    "react/jsx-no-bind": 0,
    "no-unused-vars": 1,
    "no-return-assign": 0,
    "react/sort-comp": 0
  }
}
